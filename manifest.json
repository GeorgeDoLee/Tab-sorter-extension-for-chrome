{
    "manifest_version": 3,
    "name": "Tab Sorter",
    "version": "1.0",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "./src/scripts/background.js"
    },
    "action": {
      "default_popup": "./src/popup.html",
      "default_icon": {
        "16": "./src/images/icon-16.png",
        "32": "./src/images/icon-32.png",
        "48": "./src/images/icon-48.png",
        "128": "./src/images/icon-128.png"
      }
    }
}
  